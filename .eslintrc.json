{
    "root": true,
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "react-refresh"
    ],
    "rules": {
        "react-refresh/only-export-components": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "@typescript-eslint/no-unused-vars": "off"
    },
    "ignorePatterns": [
        "dist",
        ".next",
        "node_modules"
    ]
}